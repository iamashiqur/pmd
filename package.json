{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "author": "Ashiqur Rahman - iamashiqur",
  "description": "iamashiqur.vercel.app",
  "private": true,
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": "npm run lint-prettier"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "prebuild": "npm run lint:check && npm run check-types && npm run prettier:check && npm run prepare",
    "start": "next start",
    "lint": "eslint",
    "check-types": "tsc --noEmit --pretty",
    "lint:check": "eslint --ignore-path .eslintignore --ext .js,.jsx,.ts,.tsx .",
    "prettier:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|jsx|ts|tsx|json)\"",
    "prettier:fix": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|ts|tsx|json)\"",
    "lint-prettier": "npm run lint:check && npm run prettier:fix",
    "prepare": "husky install"
  },
  "eslintIgnore": [
    "package.json",
    "node_modules/",
    "dist/",
    "build/"
  ],
  "dependencies": {
    "@eslint/config-array": "^0.19.0",
    "@eslint/object-schema": "^2.1.4",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.1.5",
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.7.5",
    "axios-retry": "^4.5.0",
    "clsx": "^2.1.1",
    "glob": "^11.0.0",
    "jspdf": "^2.5.2",
    "next": "14.2.4",
    "react": "^18",
    "react-data-table-component": "^7.6.2",
    "react-dom": "^18",
    "react-redux": "^9.1.2",
    "recharts": "^2.13.3",
    "rimraf": "^6.0.1",
    "sharp": "^0.33.5",
    "swiper": "^11.1.14",
    "tailwind-merge": "^2.5.4",
    "typescript-eslint": "^8.14.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@eslint/js": "^9.14.0",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "commitizen": "^4.3.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "husky": "^9.1.6",
    "inquirer": "^9.3.7",
    "jspdf-autotable": "^3.8.4",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
